{
    "name": "reaqta-api-client-javascript",
    "version": "3.5.0",
    "description": "An example JavaScript client for interacting with the ReaQta-Hive Api",
    "main": "index.js",
    "repository": "git@github.com:ReaQta/reaqta-api-client-javascript.git",
    "author": "Brett Beutell <b.beutell@reaqta.com>",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.21.1"
    },
    "devDependencies": {
        "colors": "^1.4.0",
        "concurrently": "^6.0.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.21.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-jest": "^24.2.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.3.1",
        "express": "^4.17.1",
        "husky": "^5.2.0",
        "jest": "^26.6.3",
        "jsdoc": "^3.6.6",
        "nodemon": "^2.0.7",
        "pinst": "^2.1.6"
    },
    "scripts": {
        "postinstall": "husky install",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable",
        "docs": "jsdoc -d ./docs -c ./jsdocconfig.json -R README.md -t ./jsdoc/templates/reaqta -u ./jsdoc/tutorials ./lib",
        "docs:dev": "concurrently \"nodemon\"",
        "docs:serve": "node docs-serve.js",
        "lint": "eslint \"lib/**\"",
        "smoketest:qa": "node smoketest.qa.js",
        "smoketest:local": "node smoketest.local.js",
        "test": "jest --collectCoverage",
        "testOne": "jest"
    },
    "jest": {
        "collectCoverage": false,
        "collectCoverageFrom": [
            "lib/**/*.+(ts|js)"
        ],
        "roots": [
            "<rootDir>"
        ],
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.+(ts|js)",
            "**/?(*.)+(spec|test).+(ts|js)"
        ]
    }
}
